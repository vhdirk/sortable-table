{
  "name": "sortable-table",
  "homepage": "https://github.com/stevenrskelton/sortable-table",
  "description": "Polymer Web Component that generates a sortable <table> from inlined or AJAX JSON, JSON5, and arrays.",
  "main": "sortable-table.html",
  "authors": [
    "Steven Skelton"
  ],
  "license": "MIT",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "keywords": [
    "web-component",
    "web-components",
    "polymer",
    "table",
    "grid",
    "paging",
    "sort",
    "list",
    "json5",
    "datatable",
    "datatables",
    "ajax"
  ],
  "dependencies": {
    "json5": "json5#>=0.4.0",
    "time-elements": "time-elements#>=0.4.0",
    "polymer": "Polymer/polymer#^1.0.0",
    "iron-ajax": "PolymerElements/iron-ajax#^1.0.0",
    "iron-dropdown": "PolymerElements/iron-dropdown#^1.0.0"
  },
  "devDependencies": {
    "web-component-tester": "Polymer/web-component-tester#>=2.2.6",
    "bootstrap": "bootstrap#>=3.3.1",
    "iron-jsonp-library": "polymerElements/iron-jsonp-library#>=1.0.0",
    "paper-item": "PolymerElements/paper-item#^1.0.0"
  }
}
